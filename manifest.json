{
    "name": "Save pin tab 2",
    "version": "0.1",
    "manifest_version": 2,

    "description": "Makes it possible to pin tabs permanently.",

    "permissions": [
        "tabs"
    ],

    "icons": {
        "128": "icons/128.png",
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png"
    },

    "background": {
        "scripts": ["background.js"]
    }
}

